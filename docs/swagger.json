  "paths": {
    "/login/github": {
      "post": {
        "description": "Can be used both for signing in and registration",
        "tags": [
          "User"
        ],
        "summary": "Authentification",
        "operationId": "userLogin",
        "parameters": [
          {
            "name": "Request",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/GithubLoginRequest"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/GithubLoginRequest"
            }
          }
        }
      }
    },
    "/me": {
      "get": {
        "security": [
          {
            "jwtToken": []
          }
        ],
        "description": "Returns current authorized user",
        "tags": [
          "User"
        ],
        "summary": "Get Account",
        "operationId": "userSelf",
        "responses": {
          "200": {
            "description": "Success",
            "schema": {
              "$ref": "#/definitions/User"
            }
          }
        }
      }
    }
  },
